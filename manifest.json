{
  "manifest_version": 3,
  "name": "drunk",
  "version": "1.0",
  "description": "絶対に酔わせる拡張機能。体調の悪い方、酔いやすい方はご注意ください",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "アニメーション開始"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
